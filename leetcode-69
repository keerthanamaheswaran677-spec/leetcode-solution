int mySqrt(int x) {
    if(x==0||x==1)
        return x;
    long low=1,high=x,a=0;
    while(low<=high) 
    {
        long mid=(low + high)/2;
    if(mid*mid==x)
        return mid;
    if (mid*mid<x) {
        a=mid;      
        low=mid+1;
        } 
    else {
        high=mid-1;
        }
    }
    return a;
}
